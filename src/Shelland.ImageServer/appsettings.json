{
    "AppSettings": {
        "ServerUrl": "http://localhost:55001",
        "RoutePrefix": "/img",
        "SaveOriginalFile": true
    },

    "Cors": {
        "IsEnabled": false
    },

    "Directory": {
        "WorkingDirectory": "/tmp",
        "AppDataDirectory": "/tmp"
    },

    "RateLimiting": {
        "IsEnabled": false
    },

    "WebHooks": {
        "IsEnabled": false,
        "PostUrl": "SOME_URL"
    },

    "ImageProcessing": {
        "JpegQuality": 100,
        "KeepMetadata": true
    },

    "StaticCache": {
        "CacheTimeSeconds": 2592000 // 1 month
    }
}
