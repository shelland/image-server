{
    "Common": {
        "ServerUrl": "http://localhost:55001",
        "RoutePrefix": "/img",
        "SaveOriginalFile": true,
        "IsServerEnabled": true
    },

    "OnDemandProcessing": {
        "IsEnabled": true,
        "AllowedDimensions": [ [ 800, 600 ] ],
        "CacheFolderName": "processed-cache" 
    },

    "Cors": {
        "IsEnabled": true,
        "AllowedOrigins": [] 
    },

    "Directory": {
        "WorkingDirectory": "/tmp",
        "AppDataDirectory": "/tmp"
    },

    "RateLimiting": {
        "IsEnabled": true,

        "Period": "1m",
        "RequestLimit": "120",
        "IpWhitelist": ["1.2.3.4"] 
    },

    "WebHooks": {
        "IsEnabled": false,
        "PostUrl": "SOME_URL"
    },

    "ImageProcessing": {
        "JpegQuality": 100,
        "KeepMetadata": true
    },

    "StaticCache": {
        "CacheTimeSeconds": 2592000 // 1 month
    }
}
